<xs:schema xmlns="http://sndlib.zib.de/globalDefs" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://sndlib.zib.de/globalDefs" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:simpleType name="identifier">
    <xs:annotation>
      <xs:documentation>Defines an identifier type used in all I/O formats of SNDlib.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-zA-Z0-9][a-zA-Z0-9\-_\.]*"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
